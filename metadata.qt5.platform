[Runtime]
runtime=org.kde.Qt5.Platform/x86_64/5.4.1
sdk=org.kde.Qt5.Sdk/x86_64/5.4.1

[Extension org.kde.Qt5.Platform.GL]
directory=lib/GL

[Extension org.kde.Qt5.Platform.Timezones]
directory=share/zoneinfo

[Extension org.kde.Qt5.Platform.Locale]
directory=share/runtime/locale
subdirectories=true

[Environment]
QT_PLUGIN_PATH=/self/lib/qt5/plugins:/lib/qt5/plugins
QML2_IMPORT_PATH=/self/lib/qt5/qml:/lib/qt5/qml

