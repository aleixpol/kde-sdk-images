[Runtime]
runtime=org.kde.KF5.Platform/x86_64/5.9.0
sdk=org.kde.KF5.Sdk/x86_64/5.9.0

[Extension org.kde.KF5.Platform.GL]
directory=lib/GL

[Extension org.kde.KF5.Platform.Timezones]
directory=share/zoneinfo

[Extension org.kde.KF5.Platform.Locale]
directory=share/runtime/locale
subdirectories=true

[Environment]
permittedEnvVars=QT_LOGGING_RULES,QT_LOGGING_CONF,QT_MESSAGE_PATTERN,QT_HASH_SEED,QT_DEBUG_PLUGINS,QT_STYLE_OVERRIDE 
